@using TheaterDeKoning.DataBase

@model List<Voorstelling>
@{
    ViewData["Title"] = "Calender";
}

<h1>@ViewData["Title"]</h1>
<br />
<div class="text-center">
    @foreach (var Voorstelling in Model)
    {
        @Voorstelling.naam @Voorstelling.datum @Voorstelling.Tijdvak @Voorstelling.Zaal @Voorstelling.beschrijving
        <br />
    }
</div>